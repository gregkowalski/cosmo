{
  "name": "food-market-ui",
  "version": "1.0.0",
  "private": true,
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "dependencies": {
    "amazon-cognito-auth-js": "^1.1.1",
    "amazon-cognito-identity-js": "^1.31.0",
    "aws-api-gateway-client": "^0.2.10",
    "create-react-class": "^15.6.2",
    "form-urlencoded": "^2.0.4",
    "google-maps-react": "^1.1.2",
    "invariant": "^2.2.2",
    "jwt-decode": "^2.2.0",
    "libphonenumber-js": "^0.4.49",
    "moment": "^2.20.1",
    "nuka-carousel": "^3.0.1",
    "query-string": "^5.0.1",
    "react": "^15.6.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dates": "^12.7.1",
    "react-dom": "^15.6.2",
    "react-google-autocomplete": "^1.0.15",
    "react-images": "^0.5.14",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.7.6",
    "react-show-more": "^1.1.1",
    "react-stripe-elements": "^1.3.2",
    "redux": "^3.6.0",
    "semantic-ui-react": "^0.77.2",
    "universal-cookie": "^2.1.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "gulp": "^3.9.1",
    "jest-enzyme": "^4.0.2",
    "react-scripts": "1.1.0",
    "react-test-renderer": "^15.6.2",
    "semantic-ui": "^2.2.13",
    "source-map-explorer": "^1.5.0"
  },
  "scripts": {
    "config:dev": "cp config/config.dev.js src/Config.js",
    "config:dev:greg": "cp config/config.dev.greg.js src/Config.js",
    "config:dev:jo": "cp config/config.dev.jo.js src/Config.js",
    "config:yapa": "cp config/config.yapa.js src/Config.js",
    "config:greg": "cp config/config.greg.js src/Config.js",
    "start": "npm run start:dev",
    "start:dev": "npm run config:dev; react-scripts start",
    "start:dev:greg": "npm run config:dev:greg; HTTPS=true HOST=ui-dev-greg.cosmo-test.com react-scripts start",
    "start:dev:jo": "npm run config:dev:jo; HTTPS=true HOST=ui-dev-jo.cosmo-test.com react-scripts start",
    "start:yapa": "npm run config:yapa; react-scripts start",
    "start:greg": "npm run config:greg; react-scripts start",
    "semantic:build": "(cd semantic && gulp build-css build-assets); (mkdir -p src/semantic/themes/ && cp semantic/dist/semantic.min.css src/semantic && cp -r semantic/dist/themes/default src/semantic/themes/)",
    "build": "npm run build:dev",
    "build:dev": "npm run config:dev; react-scripts build",
    "build:yapa": "npm run config:yapa; react-scripts build",
    "build:greg": "npm run config:greg; react-scripts build",
    "deploy": "npm run deploy:test",
    "deploy:test": "aws s3 sync build/ s3://cosmo-cleaners",
    "deploy:live": "aws s3 sync build/ s3://food-market-ui",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --coverage",
    "analyze": "source-map-explorer build/static/js/main.*",
    "eject": "react-scripts eject"
  }
}
