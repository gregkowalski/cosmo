  "scripts": {
    "config:dev": "cp config/config.dev.js src/Config.js",
    "config:dev:greg": "cp config/config.dev.greg.js src/Config.js",
    "config:dev:jo": "cp config/config.dev.jo.js src/Config.js",
    "config:yapa": "cp config/config.yapa.js src/Config.js",
    "config:greg": "cp config/config.greg.js src/Config.js",
    "config:test": "cp config/config.test.js src/Config.js",
    "config:prod": "cp config/config.prod.js src/Config.js",
    "start": "npm run start:dev",
    "start:dev": "npm run config:dev && react-scripts start",
    "start:dev:greg": "npm run config:dev:greg && HTTPS=true HOST=ui-dev-greg.cosmo-test.com react-scripts start",
    "start:dev:jo": "npm run config:dev:jo && HTTPS=true HOST=ui-dev-jo.cosmo-test.com react-scripts start",
    "start:dev:yapa": "npm run config:dev:yapa && HTTPS=true HOST=ui-dev-yapa.cosmo-test.com react-scripts start",
    "start:yapa": "npm run config:yapa && react-scripts start",
    "start:greg": "npm run config:greg && react-scripts start",
    "semantic:build": "(cd semantic && gulp build-css build-assets) && (mkdir -p src/semantic/themes/ && cp semantic/dist/semantic.min.css src/semantic && cp -r semantic/dist/themes/default src/semantic/themes/)",
    "build": "npm run build:dev",
    "build:dev": "npm run config:dev && react-scripts build",
    "build:yapa": "npm run config:yapa && react-scripts build",
    "build:greg": "npm run config:greg && react-scripts build",
    "build:test": "npm run config:test && react-scripts build",
    "build:prod": "npm run config:prod && react-scripts build",
    "deploy:test": "npm run build:test && scripts/deploy_test.sh",
    "deploy:prod": "npm run build:prod && scripts/deploy_prod.sh",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --coverage",
    "analyze": "source-map-explorer build/static/js/main.*",
    "eject": "react-scripts eject"
  }
}
