  "devDependencies": {
    "enzyme": "^2.7.1",
    "gulp": "^3.9.1",
    "react-scripts": "0.9.4",
    "semantic-ui": "^2.2.9",
    "yarn": "^0.27.5"
  },
  "scripts": {
    "config:dev": "cp config/config.dev.js src/Config.js",
    "config:yapa": "cp config/config.yapa.js src/Config.js",
    "config:greg": "cp config/config.greg.js src/Config.js",
    "start": "npm run start:dev",
    "start:dev": "npm run config:dev; NODE_PATH=src react-scripts start",
    "start:dev:https": "npm run config:dev; NODE_PATH=src HTTPS=true react-scripts start",
    "start:yapa": "npm run config:yapa; NODE_PATH=src react-scripts start",
    "start:greg": "npm run config:greg; NODE_PATH=src react-scripts start",
    "build": "npm run build:dev",
    "build:dev": "npm run config:dev; NODE_PATH=src react-scripts build",
    "build:yapa": "npm run config:yapa; NODE_PATH=src react-scripts build",
    "build:greg": "npm run config:greg; NODE_PATH=src react-scripts build",
    "deploy": "npm run deploy:test",
    "deploy:test": "aws s3 sync build/ s3://cosmo-cleaners",
    "deploy:live": "aws s3 sync build/ s3://food-market-ui",
    "test": "NODE_PATH=src react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --coverage",
    "eject": "react-scripts eject"
  }
}
